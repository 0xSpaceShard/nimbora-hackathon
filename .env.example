# REQUIRED Ethereum PK used to process batches.
PRIVATE_KEY=0xXXX

# REQUIRED Starknet 'PK' used on the L2 contract.
L2_RELAYER_PRIVATE_KEY=0xxxx

# REQUIRED Alchemy Ethereum RPC url.
ALCHEMY_RPC_URL=https://eth-goerli.g.alchemy.com/v2/xxx

# REQUIRED Alchemy Starknet RPC url.
L2_ALCHEMY_RPC_URL=https://starknet-goerli.g.alchemy.com/v2/XXX

# Starknet 'Address' used on the L2 contract.
L2_RELAYER_ADDRESS=0xxxx

# REQUIRED L2 Pooling manager address.
YIELD_DEX_L2_POOLING_MANAGER=0xXXX

# REQUIRED L1 Pooling manager address.
YIELD_DEX_L1_POOLING_MANAGER=0xXXX

# L2 Pooling manager starting block.
YIELD_DEX_L2_POOLING_MANAGER_START_BLOCK=934840

# Network can be 'mainnet' or 'goerli'.
NETWORK=goerli

# The strategy to deploy
STRATEGY_NAME=sdai

# Branch name.
BRANCH=develop

# Node env, can be 'development' or 'production'.
NODE_ENV=development

# @Optional Log format, can be 'json' or 'simple'.
LOG_FORMAT=simple

# Relayer port.
RELAYER_PORT=4000

# Indexer port.
INDEXER_PORT=3000

# Metrics port.
INDEXER_METRICS_PORT=9090

# Metrics port.
RELAYER_METRICS_PORT=9091

# @Optional Relayer log level.
LOG_LEVEL=info

# Signer service can be 'PK' or 'KMS'.
SIGNER_MODE=PK

# @Optional The graphql URL
